Page{
  height: 100%;
}
.wholeContainer {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  width:90%;
  height: 100%;
  margin-left: 5%;
}

.scrollView {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  width:90%;
  height: 80%;
  margin-left: 5%;
}
.scrollView {
  height: 100%;
}
.listCell {
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid gainsboro;
  width: 100%;
  height: 40px;
}
.titleLabel {
  flex-grow: 5;  
}

.notUniqueContainer{
  margin-top: 50px;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.notUniqueTitle{
  font-size: 13px;
  height: 100%;
}
.notUniqueButton{
  width: 70%; 
  text-align: center;
}
.participantContainerView {
  margin-top: 30px;
}
.participantWrapper {
  width: 100%;
  height: 50rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  background-color: red;
}
/* .participantTitleView{
  font-size: 13px;
  text-align: center;
} */
.participantContainer {
   height: 100%;  
  width: 50rpx;
  background-color: black;
}
.participantImage {
  background-color: blue;
  width: 50rpx;
  height: 100%;
  border-radius: 24px;
  border-width: 1px;
}

.deadLineView{
  margin-top: 5px;
  display: flex;
  justify-content: flex-start;
  font-size: 10px;
  color: gainsboro;
}